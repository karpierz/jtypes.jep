include README.rst
include CHANGES.rst
include LICENSE
include tox.ini

graft src
prune src/jt/jep/_jep/_typehandler
include src/jt/jep/_jep/_typehandler/__init__.py
recursive-exclude src/jt/jep/_java *.java

include bin/jep
include bin/jep.bat

graft docs
prune docs/_build
